<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netcoreapp3.0</TargetFrameworks>
    <PackageId>Thor.Core.Session.Abstractions</PackageId>
    <AssemblyName>Thor.Core.Session.Abstractions</AssemblyName>
    <RootNamespace>Thor.Core.Session.Abstractions</RootNamespace>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Description>Provides abstractions for the session.</Description>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Transmission.Abstractions\Transmission.Abstractions.csproj" />
  </ItemGroup>

</Project>
